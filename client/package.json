    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint-plugin-flowtype": "^2.42.0",
    "flow-bin": "^0.64.0",
